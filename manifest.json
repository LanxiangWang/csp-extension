{
    "manifest_version": 2,
    "name": "CSP",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "<all_urls>"
        
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery.min.js", "js/content_script.js"],
            "run_at": "document_start"
        }
    ]
}